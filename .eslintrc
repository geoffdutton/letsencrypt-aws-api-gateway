{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "arrowFunctions": true,
    "blockBindings": true,
    "generators": true
  },
  "rules": {
    "strict": 0,
    "no-whitespace-before-property": 2,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 0,
    "no-unused-vars": 1,
    "curly": 2,
    "no-multi-spaces": 1,
    "key-spacing": 2,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 1,
    "no-comma-dangle": 0,
    "no-use-before-define": 1,
    "no-empty": 0,
    "new-parens": 2,
    "no-cond-assign": 1,
    "quotes": [2, "single", "avoid-escape"],
    "camelcase": 0,
    "new-cap": [1, { "capIsNew": false }],
    "no-undef": 2,
    "indent": [2, 4, {"SwitchCase": 1}]
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "globals": {
  }
}
